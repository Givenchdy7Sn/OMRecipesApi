{
    "compilerOptions": {
      "module": "commonjs",
      "outDir": "./dist" ,
      "experimentalDecorators": true, 
      "emitDecoratorMetadata": true, 
      "esModuleInterop": true,
      "strict": false,
      "sourceMap": true,
      "types": ["reflect-metadata"],
      "noImplicitAny": false,
      "moduleResolution": "node",
      "target": "es6",
      "forceConsistentCasingInFileNames": true 
    },
    "lib": ["es2015"]
  }